    {
      "name": "Markdown - Plain",
      "scope": [
        "text.html.markdown",
        "punctuation.definition.list_item.markdown"
      ],
      "settings": {
        "foreground": "#a2aabc"
      }
    },
    {
      "name": "Markdown - Markup Raw Inline",
      "scope": [
        "text.html.markdown markup.raw.inline",
        "text.html.markdown markup.inline.raw.string.markdown"
      ],
      "settings": {
        "foreground": "#5ccfe6"
      }
    },
    {
